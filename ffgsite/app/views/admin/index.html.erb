<h1>Select User:</h1>
<table>
  <tr>
    <td>Phone Number</td>
    <td># Completed</td>
    <td># Total</td>
    <td>Done?</td>
    <td>Qualifies?</td>
  </tr>
  <% @users.each do |u| %>
    <tr>
      <td><%= u.phone_number %>[<%= link_to 'I', admin_path << "/user?id=" << u.link %>, <%= link_to 'A', admin_path << "/allPosts?id=" << u.link %>]</td>
      <td><%= @userMap[u][:numComp] %></td>
      <td><%= @userMap[u][:numTot] %></td>
      <td><%= (@userMap[u][:done]? 'Yes' : 'No') %></td>
      <td><%= (@userMap[u][:qualified]? 'Yes' : 'No') %></td>
    </tr>
  <% end %>
</table>
<br/>
<br/>
<%= link_to 'Go to User View', users_path %>
